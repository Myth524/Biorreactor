<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="850.0" styleClass="dashboard_container" stylesheets="@../../Styles/Synoptic.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.biorreactor.Controllers.Configuration.SummaryController">
    <VBox prefHeight="50.0" prefWidth="850.0" styleClass="title_container" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Configurations" />
    </VBox>
    <Text layoutX="368.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="alt_title_container" text="Summary">
        <font>
            <Font name="System Bold" size="12.0" />
        </font>
    </Text>
    <TableView fx:id="alarms_table" layoutX="25.0" layoutY="150.0" prefHeight="525.0" prefWidth="800.0">
        <columns>
            <TableColumn fx:id="loopNameCol" prefWidth="160.0" text="Loop Name" />
            <TableColumn fx:id="pvCol" prefWidth="160.0" text="Process Value" />
            <TableColumn fx:id="stCol" minWidth="9.0" prefWidth="160.0" text="Set Point" />
            <TableColumn fx:id="controlCol" prefWidth="160.0" text="Control Mode" />
            <TableColumn fx:id="unitsCol" prefWidth="160.0" text="Units" />
        </columns>
    </TableView>
</AnchorPane>
